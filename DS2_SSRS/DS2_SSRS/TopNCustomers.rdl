      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>c02ab2b0-7f60-44d6-a0fb-a2e6f125a2c4</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DSet_Main">
      <Query>
        <DataSourceName>DS2_DWH</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Gender">
            <Value>=Parameters!Gender.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FromAge">
            <Value>=Parameters!FromAge.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToAge">
            <Value>=Parameters!ToAge.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Region">
            <Value>=Parameters!Region.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Country">
            <Value>=Parameters!Country.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Category">
            <Value>=Parameters!Category.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT 
	[Customer].[FullName]
	, COUNT(OrderID) as NumberOfOrders
	, SUM(TotalAmount) as TotalSales
	, [Customer].[Gender]
	, [Customer].[Age]
	, [Location].[Region]
	, [Location].[Country]
	, [Product].[Category]
FROM [dbo].[FactOrder] 
JOIN [dbo].[Customer] ON [FactOrder].[CustomerID] = [Customer].[CustomerID] 
JOIN [dbo].[Location] ON [FactOrder].[LocationID] = [Location].[LocationID]
JOIN [dbo].[Product] ON [FactOrder].[ProductID] = [Product].[ProductID]
WHERE 
	[Customer].[Gender] IN (@Gender) 
	AND [Customer].[Age] BETWEEN @FromAge AND @ToAge
	AND [Location].[Region] IN (@Region) 
	AND [Location].[Country] IN (@Country)
	AND [Product].[Category] IN (@Category)
GROUP BY 
	[Customer].[FullName]
	, [Customer].[Gender]
	, [Customer].[Age]
	, [Location].[Region]
	, [Location].[Country]
	, [Product].[Category];</CommandText>
      </Query>
      <Fields>
        <Field Name="FullName">
          <DataField>FullName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="NumberOfOrders">
          <DataField>NumberOfOrders</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TotalSales">
          <DataField>TotalSales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Gender">
          <DataField>Gender</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Age">
          <DataField>Age</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
        <Field Name="Region">
          <DataField>Region</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Country">
