  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DS2_DWH</DataSourceName>
        <CommandText>SELECT        Product.Category, sum(FactOrder.TotalAmount) as SalesAmount
FROM            FactOrder INNER JOIN
                         Product ON FactOrder.ProductID = Product.ID
GROUP BY Product.Category
ORDER BY SalesAmount DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
