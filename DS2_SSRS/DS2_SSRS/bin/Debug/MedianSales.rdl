  <DataSources>
    <DataSource Name="DS2_DWH">
      <DataSourceReference>DS2_DWH</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>263509e0-1836-4675-9aa5-87b0c0b036a6</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DS2_DWH</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@MonthParam">
            <Value>=Parameters!MonthParam.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CountryParam">
            <Value>=Parameters!CountryParam.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT      Product.Category, Location.Country, avg(FactOrder.TotalAmount) MedianSales, Date.TheMonthName
 FROM  Date INNER JOIN
                        FactOrder ON Date.DateID = FactOrder.DateID INNER JOIN
                        Product ON FactOrder.ProductID = Product.ID INNER JOIN
                        Location ON FactOrder.LocationID = Location.LocationID
WHERE Date.TheMonthName IN (@MonthParam) AND Location.Country IN (@CountryParam)
GROUP BY Product.Category, Location.Country, Date.TheMonthName, Date.TheMonth
ORDER BY Date.TheMonth</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Category">
          <DataField>Category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Country">
          <DataField>Country</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MedianSales">
          <DataField>MedianSales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TheMonthName">
          <DataField>TheMonthName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="MonthParam">
      <Query>
        <DataSourceName>DS2_DWH</DataSourceName>
        <CommandText>SELECT DISTINCT TheMonthName, TheMonth
FROM            Date
ORDER BY TheMonth</CommandText>
      </Query>
      <Fields>
        <Field Name="TheMonthName">
          <DataField>TheMonthName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TheMonth">
          <DataField>TheMonth</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="CountryParam">
      <Query>
        <DataSourceName>DS2_DWH</DataSourceName>
        <CommandText>SELECT DISTINCT Country
FROM            Location
ORDER BY Country</CommandText>
      </Query>
      <Fields>
        <Field Name="Country">
          <DataField>Country</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
